Feature('csssr.com')

Scenario('Переход на страницу вакансий', ({ I }) => {
  I.say('переходим на csssr.com')
  I.amOnPage('https://csssr.com')

  I.say('кликаем на ссылку «Вакансии»')
  I.click('[data-testid="Header:link.careers"]')

  I.say('перешли на страницу вакансий')
  I.seeInCurrentUrl('https://csssr.com/en-us/jobs')
})
